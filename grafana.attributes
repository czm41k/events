{{- define "text"}}
  {{printf "{\"%s\":\"\",\"%s\":\"\"}" .type .channel}}
{{- end}}
{{- define "vcenter"}}
  {{printf "{\"%s\":\"\"," .type}}
  {{printf "\"%s\":\"\"," .channel}}
  {{printf "\"%s\":\"\"," .data.Subject }}
  {{printf "\"%s\":\"\"" .data.DestLocation}}
  {{printf "}"}}
{{- end}}
{{- define "grafana-attributes"}}
  {{- if .data}}
    {{- if eq .type "vcenterEvent"}}
      {{template "vcenter" .}}
    {{- else }}
      {{template "text" .}}
    {{- end }}
  {{end}}
{{- end}}

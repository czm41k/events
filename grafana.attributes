{{- define "text"}}
  {{printf "{\"%s\":\"\",\"%s\":\"\"}" .type .channel}}
{{- end}}
{{- define "vcenter"}}
  {{printf "{\"%s\":\"\",\"%s\":\"\",\"%s\":\"\",\"%s\":\"\"}" .type .channel .data.subject .data.data.ComputeResource.Name }}
{{- end}}
{{- define "grafana-attributes"}}
  {{- if .data}}
    {{- if eq .type "vcenterEvent"}}
      {{template "vcenter" .}}
    {{- else }}
      {{template "text" .}}
    {{- end }}
  {{end}}
{{- end}}
